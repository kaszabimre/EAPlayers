{
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchFileNames": ["**/libs.versions.toml"],
      "groupName": "TOML version catalog updates"
    },
    {
      "matchFileNames": [".github/workflows/*.yml"],
      "groupName": "GitHub Actions updates",
      "matchManagers": ["github-actions"]
    }
  ],
  "schedule": ["at any time"],
  "automerge": false,
  "labels": [
    "dependency-update"
  ]
}